<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.CanyindianDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.CanyindianEntity" id="canyindianMap">
        <result property="canyindianzhanghao" column="canyindianzhanghao"/>
        <result property="mima" column="mima"/>
        <result property="canyindianmingcheng" column="canyindianmingcheng"/>
        <result property="tupian" column="tupian"/>
        <result property="dizhi" column="dizhi"/>
        <result property="canyindianxiangqing" column="canyindianxiangqing"/>
        <result property="lianxifangshi" column="lianxifangshi"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.CanyindianVO" >
		SELECT * FROM canyindian  canyindian         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.CanyindianVO" >
		SELECT  canyindian.* FROM canyindian  canyindian
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.CanyindianView" >

		SELECT  canyindian.* FROM canyindian  canyindian 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.CanyindianView" >
		SELECT * FROM canyindian  canyindian <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	
</mapper>